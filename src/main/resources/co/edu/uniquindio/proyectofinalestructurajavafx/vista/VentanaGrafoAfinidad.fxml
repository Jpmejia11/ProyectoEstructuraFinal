<?xml version="1.0" encoding="UTF-8"?>
 
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="co.edu.uniquindio.proyectofinalestructurajavafx.controlador.GrafoAfinidadController"
            prefWidth="800" prefHeight="600">

    <ScrollPane fx:id="scrollPane" fitToWidth="true" fitToHeight="true" AnchorPane.topAnchor="0"
                AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0">
        <content>
            <Pane fx:id="pane" prefWidth="800" prefHeight="600" />
        </content>
    </ScrollPane>

</AnchorPane>
